				"next":"new"
			},
			"new": {
				"icon":"fa-star",
				"color":"black",
				"next":"yes"
			},
			"yes": {
				"icon":"fa-check",
				"color":"black",
				"next":"del"
			},
			"del": {
				"icon":"fa-remove",
				"color":"black",
				"next":"no"
			}
