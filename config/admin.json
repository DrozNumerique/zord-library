{
	"tabs": {
		"import":  {
			"scope": "context",
			"scripts": [
				{
					"src": "/library/js/admin/import.js",
					"type": "text/javascript"
				}
			]
		},
		"publish":  {
			"scope": "context",
			"scripts": [
				{
					"src": "/library/js/admin/publish.js",
					"type": "text/javascript"
				}
			]
		}
	},
	"switches": {
		"select": {
			"no": {
				"icon":"fa-check",
				"color":"white",
				"next":"new"
			},
			"new": {
				"icon":"fa-star",
