			}
		},
		"manage": {
			"no": {
				"display":"hidden",
				"next":"new"
			},
			"new": {
				"display":"fa-star",
				"next":"yes"
			},
			"yes": {
				"display":"fa-check",
				"next":"del"
			},
			"del": {
				"display":"fa-remove",
				"next":"no"
			}
		}
