{
	"extensions": {
		"Word2007": "docx",
		"ODText": "odt",
		"RTF": "rtf",
		"HTML": "html",
		"PDF": "pdf"
	},
	"layouts": {
		"default": {
			"height": "21cm",
			"width": "15cm",
			"margin": {
				"top": "2cm",
				"bottom": "2cm",
				"left": "2cm",
				"right": "2cm"
			}
		}
	},
	"images": {
		"default": {
			"alignment": "center",
			"height": "10cm"
		},
		"#(/){0,1}logo_publisher\\.(\\w)+#i": {
			"alignment": "center",
			"width": "5cm"
		}
	},
	"styles": {
		"font": {
			"*": {
				"name": "Times new roman",
				"size": 10
			},
			"*@note": {
				"size": 8
			},
			"*@header": {
				"size": 9
			},
			"*@footer": {
				"size": 9
			},
			"pb": {
				"size": 8
			},
			"emph": {
				"italic": true
			},
			"hi.sup": {
				"superScript": true
			},
			"hi.sub": {
				"subScript": true
			},
			"hi.sc": {
				"smallCaps": true
			},
			"hi.i": {
				"italic": true
			},
			"stage": {
				"italic": true
			}
		},
		"paragraph": {
			"*@text": {
				"alignment": "both"
			},
			"*@header": {
				"alignment": "center"
			},
			"*@footer": {
				"alignment": "center"
			},
			"*.center": {
				"alignment": "center"
			},
			"*.#center": {
				"alignment": "center"
			},
			"*.left": {
				"alignment": "left"
			},
			"*.#left": {
				"alignment": "left"
			},
			"*.right": {
				"alignment": "right"
			},
			"*.#right": {
				"alignment": "right"
			},
			"graphic": {
				"alignment": "center"
			},
			"titlePage": {
				"alignment": "center"
			},
			"titlePart": {
				"space": {
					"after": "0.5cm"
				}
			},
			"argument.rights": {
				"space": {
					"before": "0.5cm"
				}
			},
			"head": {
				"alignment": "center",
				"space": {
					"after": "0.5cm"
				}
			},
			"p": {
				"indentation": {
					"firstLine": "1cm"
				}
			},
			"p.center": {
				"indentation": {
					"firstLine": 0
				}
			},
			"p.#center": {
				"indentation": {
					"firstLine": 0
				}
			},
			"l": {
				"indentation": {
					"firstLine": "1cm"
				}
			},
			"speaker": {
				"alignment": "center"
			},
			"caption": {
				"alignment": "center"
			},
			"epigraph": {
				"alignment": "right",
				"indentation": {
					"left": "3cm"
				}
			}
		}
	},
	"paragraph": [
		"p",
		"l",
		"bibl",
		"speaker",
		"stage",
		"item",
		"emph",
		"label",
		"titlePart",
		"caption",
		"table"
	]
}