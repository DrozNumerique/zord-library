{
	"extension": {
		"Word2007": "docx",
		"ODText": "odt",
		"RTF": "rtf",
		"HTML": "html",
		"PDF": "pdf"
	},
	"layout": {
		"default": {
			"height": "21cm",
			"width": "15cm",
			"margin": {
				"top": "2cm",
				"bottom": "2cm",
				"left": "2cm",
				"right": "2cm"
			}
		}
	},
	"image": {
		"default": {
			"alignment": "center",
			"margin": "0.5cm",
			"scale": "fit"
		}
	},
	"table": {
		"default": {
			"alignment": "center",
			"layout": "fixed"
		}
	},
	"row": {
		"default": {
			"cantSplit": true
		}
	},
	"cell": {
		"default": {
			"valign": "center"
		}
	},
	"font": {
		"*": {
			"name": "Times new roman",
			"size": 10
		},
		"*@note": {
			"size": 8
		},
		"*@header": {
			"size": 9
		},
		"*@footer": {
			"size": 9
		},
		"pb": {
			"size": 8
		},
		"head": {
			"size": 13
		},
		"emph": {
			"italic": true
		},
		"hi.sup": {
			"superScript": true
		},
		"hi.sub": {
			"subScript": true
		},
		"hi.sc": {
			"smallCaps": true
		},
		"hi.i": {
			"italic": true
		},
		"stage": {
			"italic": true
		}
	},
	"paragraph": {
		"names": [
			"p",
			"l",
			"bibl",
			"speaker",
			"stage",
			"item",
			"emph",
			"label",
			"titlePart",
			"caption",
			"table",
			"cell",
			"quote"
		],
		"*": {
			"contextualSpacing": true
		},
		"*@text": {
			"alignment": "both"
		},
		"*@header, *@footer, *.center, *.#center, graphic, titlePage, speaker, caption, cell": {
			"alignment": "center"
		},
		"*.left, *.#left": {
			"alignment": "left"
		},
		"*.right, *.#right": {
			"alignment": "right"
		},
		"titlePart": {
			"space": {
				"after": "0.5cm"
			}
		},
		"argument.rights": {
			"space": {
				"before": "0.5cm"
			}
		},
		"head": {
			"alignment": "center",
			"space": {
				"after": "0.5cm"
			}
		},
		"div > p, quote > p, l": {
			"indentation": {
				"firstLine": "1cm"
			}
		},
		"div > p.center, div > p.#center": {
			"indentation": {
				"firstLine": 0
			}
		},
		"epigraph": {
			"alignment": "right",
			"indentation": {
				"left": "3cm"
			},
			"space": {
				"before": "0.25cm",
				"after": "0.5cm"
			}
		},
		"quote": {
			"indentation": {
				"left": "1cm"
			},
			"space": {
				"before": "0.25cm",
				"after": "0.25cm"
			}
		}
	}
}
