{
	"extensions": {
		"Word2007": "docx",
		"ODText": "odt",
		"RTF": "rtf",
		"HTML": "html",
		"PDF": "pdf"
	},
	"layouts": {
		"default": {
			"height": "21cm",
			"width": "15cm",
			"margin": {
				"top": "2cm",
				"bottom": "2cm",
				"left": "2cm",
				"right": "2cm"
			}
		}
	},
	"styles": {
		"font": {
			"*": {
				"name": "Times new roman",
				"size": 10
			},
			"*@note": {
				"size": 8
			},
			"*@header": {
				"size": 9
			},
			"*@footer": {
				"size": 9
			},
			"emph": {
				"italic": true
			},
			"hi.sup": {
				"superScript": true
			},
			"hi.sub": {
				"subScript": true
			},
			"hi.sc": {
				"smallCaps": true
			},
			"hi.i": {
				"italic": true
			},
			"stage": {
				"italic": true
			}
		},
		"paragraph": {
			"*@header": {
				"alignment": "center"
			},
			"*@footer": {
				"alignment": "center"
			},
			"*.#center": {
				"alignment": "center"
			},
			"*.#left": {
				"alignment": "left"
			},
			"*.#right": {
				"alignment": "right"
			},
			"titlePage": {
				"alignment": "center"
			},
			"p": {
				"alignment": "both",
				"indentation": {
					"firstLine": "1cm"
				}
			},
			"p.#center": {
				"alignment": "center",
				"indentation": {
					"firstLine": 0
				}
			},
			"l": {
				"indentation": {
					"firstLine": "1cm"
				}
			},
			"speaker": {
				"alignment": "center"
			}
		}
	},
	"paragraph": [
		"p",
		"l",
		"bibl",
		"speaker",
		"stage",
		"item",
		"emph",
		"label"
	]
}